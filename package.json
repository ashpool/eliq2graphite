{
  "name": "eliq2graphite",
  "version": "2.0.0",
  "description": "Fetches energy consumption from Eliq and forwards it to Graphite",
  "keywords": [
    "eliq",
    "graphite"
  ],
  "homepage": "https://github.com/ashpool/eliq2graphite",
  "bugs": {
    "url": "https://github.com/ashpool/eliq2graphite/issues"
  },
  "license": "MIT",
  "author": "Magnus Ljadas <magnus.ljadas@gmail.com> (https://github.com/ashpool)",
  "contributors": [
    {
      "name": "Magnus Ljadas <magnus.ljadas@gmail.com> (https://github.com/ashpool)"
    }
  ],
  "files": [
    "build",
    "bin"
  ],
  "main": "bin/eliq2graphite",
  "bin": {
    "eliq2graphite": "./bin/eliq2graphite.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashpool/eliq2graphite.git"
  },
  "scripts": {
    "build": "tsc",
    "eslint:fix": "eslint **/*.ts --fix",
    "test": "eslint **/*.ts && mocha -r ts-node/register test/**/*test.ts",
    "prettier": "prettier --write '{*/**/*,*}.{js,md,json,d.ts}'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,md,json,d.ts}": [
      "npm run prettier",
      "git add"
    ]
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "eliq-promise": "^2.0.4",
    "graphite-promise": "^2.0.10",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "sinon": "^7.1.1",
    "just-extend": ">=4.0.0",
    "@types/chai": "^4.2.2",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "ts-mocha": "^6.0.0",
    "ts-mockito": "^2.4.2",
    "typescript": "^3.6.3"
  }
}
