{
  "name": "eliq2graphite",
  "version": "2.0.0",
  "description": "Fetches energy consumption from Eliq and forwards it to Graphite",
  "keywords": [
    "eliq",
    "graphite"
  ],
  "homepage": "https://github.com/ashpool/eliq2graphite",
  "bugs": {
    "url": "https://github.com/ashpool/eliq2graphite/issues"
  },
  "license": "MIT",
  "author": "Magnus Ljadas <magnus.ljadas@gmail.com> (https://github.com/ashpool)",
  "contributors": [
    {
      "name": "Magnus Ljadas <magnus.ljadas@gmail.com> (https://github.com/ashpool)"
    }
  ],
  "files": [
    "build"
  ],
  "main": "build/eliq2graphite",
  "bin": {
    "eliq2graphite": "./build/eliq2graphite.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ashpool/eliq2graphite.git"
  },
  "scripts": {
    "build": "tsc",
    "eslint:fix": "eslint **/*.ts --fix",
    "test": "eslint **/*.ts && mocha -r ts-node/register test/**/*test.ts",
    "prettier": "prettier --write '{*/**/*,*}.{js,md,json,d.ts}'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,md,json,d.ts}": [
      "npm run prettier",
      "git add"
    ]
  },
  "preferGlobal": true,
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "eliq-promise": "^3.1.0",
    "graphite-promise": "^2.1.1",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "@types/sinon": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "just-extend": "^4.1.1",
    "mocha": "^8.2.1",
    "prettier": "^2.2.0",
    "sinon": "^9.2.1",
    "ts-mocha": "^8.0.0",
    "ts-mockito": "^2.6.1",
    "ts-sinon": "^2.0.1",
    "typescript": "^4.1.2"
  }
